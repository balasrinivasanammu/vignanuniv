
num = int(input("Enter a number: "))


def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True


reverse_num = int(str(num)[::-1])

if is_prime(num) and is_prime(reverse_num):
    print("Magical prime number")
else:
    print("Not a magical prime number")
