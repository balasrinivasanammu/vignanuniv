
num = int(input("Enter a number: "))


def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

next_num = num + 1
while True:
    if is_prime(next_num):
        print("Next prime number is:", next_num)
        break
    next_num += 1
